    function setHookFeeEnabled(PoolId id, bool enabled) external onlyRole(FEE_MANAGER_ROLE) \{\
        _validatePool(id);\
        if (hookFeeConfigurations[id].enabled == enabled) \{\
            return;\
        \}\
